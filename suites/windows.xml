<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Windows 10 Smoke Test" verbose="1">
    <parameter name="os" value="Windows" />
    <parameter name="os_version" value="10" />
    <parameter name="browser_version" value="latest" />
    <test name="Internet Explorer 11">
        <parameter name="browser" value="IE" />
        <parameter name="browser_version" value="11.0" />
        <classes>
            <class name="page.SumUpLoginTest" />
        </classes>
    </test>
    <test name="Edge Latest">
        <parameter name="browser" value="Edge" />
        <classes>
            <class name="page.SumUpLoginTest" />
        </classes>
    </test>
    <test name="Chrome Latest">
        <parameter name="browser" value="Chrome" />
        <classes>
            <class name="page.SumUpLoginTest" />
        </classes>
    </test>
    <test name="Firefox Latest">
        <parameter name="browser" value="Firefox" />
        <classes>
            <class name="page.SumUpLoginTest" />
        </classes>
    </test>
</suite>
